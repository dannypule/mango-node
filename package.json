{
  "name": "mango-node",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "npm run serve:prod",
    "postinstall": "npm run build-ts",
    "serve:prod": "node dist/server.js",
    "serve": "nodemon dist/server.js",
    "dev": "npm run build && npm run watch",
    "debug": "npm run build && npm run watch-debug",
    "setup:db": "node dist/dev-setup/db.setup.js",
    "build": "rm -rf dist && npm run build-ts && npm run tslint",
    "watch": "concurrently -k -p \"[{name}]\" -n \"TypeScript,Node\" -c \"cyan.bold,green.bold\" \"npm run watch-ts\" \"npm run serve\"",
    "test": "jest --forceExit --coverage --verbose",
    "build-ts": "tsc",
    "watch-ts": "tsc -w",
    "tslint": "tslint -c tslint.json -p tsconfig.json",
    "serve-debug": "nodemon --inspect dist/server.js",
    "watch-debug": "concurrently -k -p \"[{name}]\" -n \"TypeScript,Node\" -c \"cyan.bold,green.bold\" \"npm run watch-ts\" \"npm run serve-debug\""
  },
  "engines": {
    "node": "6.11.4"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dannypule/mango-node.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/dannypule/mango-node/issues"
  },
  "homepage": "https://github.com/dannypule/mango-node#readme",
  "dependencies": {
    "bcrypt": "^1.0.3",
    "cors": "^2.8.4",
    "dotenv": "^4.0.0",
    "express": "^4.16.2",
    "faker": "^4.1.0",
    "jsonwebtoken": "^8.1.0",
    "moment": "^2.19.3",
    "pg": "^7.3.0",
    "pg-hstore": "^2.3.2",
    "sequelize": "^4.17.2",
    "uuid-token-generator": "^1.0.0"
  },
  "devDependencies": {
    "@types/bcrypt": "^1.0.0",
    "@types/body-parser": "^1.16.8",
    "@types/cors": "^2.8.1",
    "@types/dotenv": "^4.0.2",
    "@types/express": "^4.0.37",
    "@types/faker": "^4.1.1",
    "@types/jest": "^22.0.1",
    "@types/node": "^9.3.0",
    "@types/passport": "^0.4.2",
    "@types/passport-local": "^1.0.32",
    "@types/request": "^2.0.12",
    "@types/moment": "^2.13.0",
    "@types/sequelize": "^4.0.77",
    "@types/supertest": "^2.0.4",
    "concurrently": "^3.5.0",
    "jest": "^22.1.4",
    "nodemon": "^1.12.1",
    "shelljs": "^0.7.8",
    "supertest": "^3.0.0",
    "ts-jest": "^22.0.1",
    "tslint": "^5.8.0",
    "typescript": "^2.5.3"
  }
}
