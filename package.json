{
  "name": "mango-node",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "node dist/server.js",
    "dev": "nodemon src/server.js --exec babel-node",
    "local": "NODE_ENV=local nodemon src/server.js --exec babel-node",
    "build": "babel src -d dist",
    "postinstall": "npm run build",
    "setup:db": "npm run build && node dist/dev-setup/db.setup.js",
    "setup:dblocal":
      "npm run build && NODE_ENV=local node dist/dev-setup/db.setup.js",
    "jest": "jest --forceExit --coverage --verbose",
    "lint": "eslint src",
    "lint:fix": "eslint --fix src"
  },
  "jest": {
    "transform": {},
    "verbose": true
  },
  "engines": {
    "node": "6.11.4"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dannypule/mango-node.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/dannypule/mango-node/issues"
  },
  "homepage": "https://github.com/dannypule/mango-node#readme",
  "dependencies": {
    "bcrypt": "1.0.3",
    "cors": "2.8.4",
    "dotenv": "4.0.0",
    "express": "4.16.2",
    "faker": "4.1.0",
    "jsonwebtoken": "8.2.0",
    "moment": "2.19.3",
    "morgan": "1.9.0",
    "parse-error": "^0.2.0",
    "passport": "0.4.0",
    "passport-jwt": "4.0.0",
    "pg": "7.3.0",
    "pg-hstore": "2.3.2",
    "sequelize": "4.17.2"
  },
