{
  "name": "mango-node",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "node dist/server.js",
    "dev": "NODE_ENV=development nodemon src/server.js --exec babel-node",
    "dev:staging": "NODE_ENV=staging nodemon src/server.js --exec babel-node",
    "build": "rm -rf dist && babel src -d dist",
    "postinstall": "npm run build",
    "db:setup": "NODE_ENV=development babel-node src/db_setup",
    "db:setup:staging": "NODE_ENV=staging babel-node src/db_setup",
    "db": "docker-compose -f docker-compose.yml up",
    "test": "NODE_ENV=development jest --watch",
    "test:c": "jest --forceExit --coverage --verbose",
    "lint": "eslint src",
    "lint:fix": "eslint --fix src"
  },
  "jest": {
    "transform": {},
    "verbose": true
  },
  "engines": {
    "node": "8.11.2"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dannypule/mango-node.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/dannypule/mango-node/issues"
  },
  "homepage": "https://github.com/dannypule/mango-node#readme",
  "dependencies": {
    "bcrypt": "1.0.3",
    "colors": "^1.3.0",
    "cors": "2.8.4",
    "dotenv": "4.0.0",
    "express": "4.16.2",
    "faker": "4.1.0",
    "jsonwebtoken": "8.2.0",
    "moment": "2.19.3",
    "morgan": "1.9.0",
    "parse-error": "0.2.0",
    "passport": "0.4.0",
    "passport-jwt": "4.0.0",
    "pg": "7.4.3",
    "pg-hstore": "2.3.2",
    "sequelize": "4.38.0",
    "yup": "0.24.1"
  },
  "devDependencies": {
    "axios": "^0.18.0",
    "babel-cli": "6.26.0",
    "babel-core": "6.26.0",
    "babel-eslint": "8.2.2",
    "babel-jest": "22.4.1",
    "babel-plugin-transform-async-to-generator": "6.24.1",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "6.26.0",
    "babel-plugin-transform-runtime": "6.23.0",
    "babel-polyfill": "6.26.0",
    "babel-preset-env": "1.6.1",
    "babel-register": "6.26.0",
    "chai": "^4.1.2",
    "chai-http": "^4.0.0",
    "concurrently": "3.5.0",
    "eslint": "4.19.1",
    "eslint-config-airbnb": "16.1.0",
    "eslint-config-strongloop": "2.1.0",
    "eslint-plugin-import": "2.10.0",
    "eslint-plugin-jsx-a11y": "6.0.3",
    "eslint-plugin-react": "7.7.0",
    "jest": "22.1.4",
    "mocha": "5.0.4",
    "nodemon": "1.17.1",
    "regenerator-runtime": "0.11.1",
    "shelljs": "0.7.8",
    "supertest": "3.0.0"
  }
