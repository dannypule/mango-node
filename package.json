{
  "name": "mango-node",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "node dist/server.js",
    "dev": "nodemon src/server.js --exec babel-node",
    "build": "babel src -d dist",
    "postinstall": "npm run build",
    "debug": "npm run build && npm run watch-debug",
    "setup:db": "node dist/dev-setup/db.setup.js",
    "jest": "jest --forceExit --coverage --verbose",
    "copy-static-assets": "node copyStaticAssets.js",
    "serve-debug": "nodemon --inspect dist/server.js",
    "watch-debug":
      "concurrently -k -p \"[{name}]\" -n \"TypeScript,Node\" -c \"cyan.bold,green.bold\" \"npm run watch-ts\" \"npm run serve-debug\""
  },
  "jest": {
    "transform": {},
    "verbose": true
  },
  "engines": {
    "node": "6.11.4"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dannypule/mango-node.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/dannypule/mango-node/issues"
  },
  "homepage": "https://github.com/dannypule/mango-node#readme",
  "dependencies": {
    "bcrypt": "^1.0.3",
    "cors": "^2.8.4",
    "dotenv": "^4.0.0",
    "express": "^4.16.2",
    "faker": "^4.1.0",
    "moment": "^2.19.1",
    "pg": "^7.3.0",
    "pg-hstore": "^2.3.2",
    "sequelize": "^4.17.2"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-jest": "^22.4.1",
    "babel-polyfill": "^6.26.0",
    "babel-preset-latest": "^6.24.1",
    "babel-register": "^6.26.0",
    "concurrently": "^3.5.0",
    "jest": "^22.1.4",
    "mocha": "^5.0.4",
    "nodemon": "^1.17.1",
    "regenerator-runtime": "^0.11.1",
    "shelljs": "^0.7.8",
    "supertest": "^3.0.0"
  }
}
