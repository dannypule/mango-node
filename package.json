{
  "name": "node-project-kit",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "postinstall": "npm run build",
    "start": "node dist/server.js",
    "local": "NODE_ENV=development nodemon src/server.js --exec babel-node",
    "staging": "NODE_ENV=staging nodemon src/server.js --exec babel-node",
    "build": "rm -rf dist && babel src -d dist",
    "db:local": "docker-compose -f docker-compose-db.yml up",
    "db:local:setup": "NODE_ENV=development babel-node src/db_setup",
    "db:staging:setup": "NODE_ENV=staging babel-node src/db_setup",
    "test": "NODE_ENV=development jest -c jest.config.unit.js --watch",
    "test:int": "NODE_ENV=development jest -c jest.config.integration.js",
    "test:cov": "jest -c jest.config.unit.js --forceExit --coverage --verbose",
    "lint": "eslint src",
    "lint:fix": "eslint --fix src"
  },
  "jest": {
    "transform": {},
    "verbose": true
  },
  "engines": {
    "node": "8.11.2"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dannypule/node-project-kit.git"
  },
  "keywords": [],
  "author": "Danny Pule",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/dannypule/node-project-kit/issues"
  },
  "homepage": "https://github.com/dannypule/node-project-kit#readme",
  "dependencies": {
    "bcrypt": "3.0.2",
    "colors": "1.3.2",
    "cors": "2.8.5",
    "dotenv": "6.1.0",
    "express": "4.16.4",
    "faker": "4.1.0",
    "http-status-codes": "1.3.0",
    "jsonwebtoken": "8.4.0",
    "moment": "2.22.2",
    "morgan": "1.9.1",
    "parse-error": "0.2.0",
    "passport": "0.4.0",
    "passport-jwt": "4.0.0",
    "pg": "7.6.1",
    "pg-hstore": "2.3.2",
    "sequelize": "4.41.2",
    "uuid": "3.3.2",
    "yup": "0.26.6"
  },
  "devDependencies": {
    "axios": "0.18.0",
    "babel-cli": "6.26.0",
    "babel-core": "6.26.3",
    "babel-eslint": "10.0.1",
    "babel-jest": "23.6.0",
    "babel-plugin-transform-async-to-generator": "6.24.1",
    "babel-plugin-transform-class-properties": "6.24.1",
    "babel-plugin-transform-object-rest-spread": "6.26.0",
    "babel-plugin-transform-runtime": "6.23.0",
    "babel-polyfill": "6.26.0",
    "babel-preset-env": "1.7.0",
    "babel-register": "6.26.0",
    "eslint": "5.8.0",
    "eslint-config-airbnb": "17.1.0",
    "eslint-config-prettier": "3.3.0",
    "eslint-plugin-import": "2.14.0",
    "eslint-plugin-jsx-a11y": "6.1.2",
    "eslint-plugin-prettier": "3.0.0",
    "eslint-plugin-react": "7.11.1",
    "jest": "23.6.0",
    "nodemon": "1.18.6",
    "prettier": "1.15.2"
  }
}
